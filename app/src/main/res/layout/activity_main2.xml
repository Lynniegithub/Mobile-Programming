<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor"
    tools:context=".Main2Activity">


    <TextView
        android:id="@+id/dfs"
        android:layout_width="240dp"
        android:layout_height="47dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:fontFamily="casual"
        android:text="면세점가격비교"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/오늘의환율"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="116dp"
        android:text="오늘의 환율: 1$ = 1,138.5원 "
        android:textColor="@android:color/background_light"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/lottedutyfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="28dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="96dp"
        android:background="@android:color/background_light"
        android:scaleType="centerCrop"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@+id/shilladutyfree"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dfs"
        app:srcCompat="@drawable/lottedutyfree" />

    <ImageButton
        android:id="@+id/shilladutyfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="96dp"
        android:background="@android:color/background_light"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toTopOf="@+id/신라면세점"
        app:layout_constraintEnd_toStartOf="@+id/shinsegaedutyfree"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toEndOf="@drawable/lottedutyfree"
        app:layout_constraintTop_toBottomOf="@+id/dfs"
        app:srcCompat="@drawable/shilladutyfree" />

    <ImageButton
        android:id="@+id/shinsegaedutyfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="36dp"
        android:background="@android:color/background_light"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toTopOf="@+id/신세계면세점"
        app:layout_constraintEnd_toStartOf="@+id/dootadutyfree"
        app:layout_constraintTop_toBottomOf="@+id/오늘의환율"
        app:srcCompat="@drawable/shinsegaedutyfree" />

    <ImageButton
        android:id="@+id/dootadutyfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        android:layout_marginTop="35dp"
        android:background="@android:color/background_light"
        android:scaleType="centerInside"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintBottom_toTopOf="@+id/두타면세점"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/오늘의환율"
        app:srcCompat="@drawable/dootadutyfree" />

    <ImageButton
        android:id="@+id/dongwhadutyfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="28dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="13dp"
        android:background="@android:color/background_light"
        android:scaleType="centerInside"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintBottom_toTopOf="@+id/동화면세점"
        app:layout_constraintEnd_toStartOf="@+id/galleriadutyfree"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/롯데면세점"
        app:srcCompat="@drawable/dongwhadutyfree" />

    <ImageButton
        android:id="@+id/galleriadutyfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="13dp"
        android:background="@android:color/background_light"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toTopOf="@+id/갤러리아면세점"
        app:layout_constraintEnd_toStartOf="@+id/smdutyfree"
        app:layout_constraintTop_toBottomOf="@+id/신라면세점"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/galleriadutyfree" />

    <ImageButton
        android:id="@+id/smdutyfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="13dp"
        android:background="@android:color/background_light"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toTopOf="@+id/sm면세점"
        app:layout_constraintEnd_toStartOf="@+id/jdcdutyfree"
        app:layout_constraintTop_toBottomOf="@+id/신세계면세점"
        app:srcCompat="@drawable/smdutyfree" />

    <ImageButton
        android:id="@+id/jdcdutyfree"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        android:layout_marginTop="13dp"
        android:background="@android:color/background_light"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toTopOf="@+id/JDC면세점"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/두타면세점"
        app:srcCompat="@drawable/jdcdutyfree" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="346dp"
        android:layout_height="232dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="380dp"
        android:background="@android:color/transparent"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/weeklyevent" />

    <TextView
        android:id="@+id/롯데면세점"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="32dp"
        android:lineSpacingExtra="8sp"
        android:text="롯데면세점"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        app:layout_constraintBaseline_toBaselineOf="@+id/신라면세점"
        app:layout_constraintEnd_toStartOf="@+id/신라면세점"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/신라면세점"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="8sp"
        android:text="신라면세점"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        app:layout_constraintBaseline_toBaselineOf="@+id/신세계면세점"
        tools:layout_editor_absoluteX="114dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/신세계면세점"
        android:layout_width="73dp"
        android:layout_height="20dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:lineSpacingExtra="8sp"
        android:text="신세계면세점"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        android:textSize="13.5sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/두타면세점"
        app:layout_constraintEnd_toStartOf="@+id/두타면세점" />

    <TextView
        android:id="@+id/두타면세점"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="8sp"
        android:text="두타면세점"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        tools:layout_editor_absoluteX="289dp"
        tools:layout_editor_absoluteY="245dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/갤러리아면세점"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:lineSpacingExtra="0sp"
        android:text="갤러리아 면세점"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        app:layout_constraintBaseline_toBaselineOf="@+id/sm면세점"
        app:layout_constraintEnd_toStartOf="@+id/sm면세점" />

    <TextView
        android:id="@+id/sm면세점"
        android:layout_width="71dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:lineSpacingExtra="8sp"
        android:text="sm면세점"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        android:textSize="13sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/JDC면세점"
        app:layout_constraintEnd_toStartOf="@+id/JDC면세점" />

    <TextView
        android:id="@+id/JDC면세점"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:lineSpacingExtra="8sp"
        android:text="JDC면세점"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        app:layout_constraintBottom_toTopOf="@+id/imageButton7"
        tools:layout_editor_absoluteX="289dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/동화면세점"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="24dp"
        android:lineSpacingExtra="8sp"
        android:text="동화면세점"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        app:layout_constraintBaseline_toBaselineOf="@+id/갤러리아면세점"
        app:layout_constraintEnd_toStartOf="@+id/갤러리아면세점"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>